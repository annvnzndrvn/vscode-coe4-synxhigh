		"kwsMonsterModding6": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(acutesenses|spiritsight|stealth|foreststealth|snowstealthy|invisible|farsight|nocombat)\\b"
			}]
		},
		"kwsMonsterModding7": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(noheal|fastheal|coldheal|regeneration|saner|voidsanity|eatvillage|minorstartaff)\\b"
			}]
		},
		"kwsMonsterModding8": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(meleeweapon|meleeweaponbonus|meleeweaponlong|rangedweapon|rangedweaponbonus|rangedweapon25|rangedweapon50|prebatweapon|assassinweapon|siegeweapon)\\b"
			}]
		},
		"kwsMonsterModding9": { 
				"patterns": [{
