
{	
	
	"scopeName": "coe4.in.vscode",	
	"name": "c4m CoE4 modding language",
	"patterns": [

		{ "include": "#kwsWeaponModding1" }, { "include": "#kwsWeaponModding2" },
		{ "include": "#kwsWeaponModding3" }, { "include": "#kwsWeaponModding4" },
		{ "include": "#kwsMonsterModding1" }, { "include": "#kwsMonsterModding2" },
		{ "include": "#kwsMonsterModding3" }, { "include": "#kwsMonsterModding4" },
		{ "include": "#kwsMonsterModding5" }, { "include": "#kwsMonsterModding6" },
		{ "include": "#kwsMonsterModding7" }, { "include": "#kwsMonsterModding8" },
		{ "include": "#kwsMonsterModding9" }, { "include": "#kwsMonsterModding10" },
		{ "include": "#kwsMonsterModding11" }, { "include": "#kwsMonsterModding12" },
		{ "include": "#kwsMonsterModding13" }, { "include": "#kwsMonsterModding14" },
		{ "include": "#kwsMonsterModding15" }, { "include": "#kwsMonsterModding16" },
		{ "include": "#kwsMonsterModding17" }, { "include": "#kwsMonsterModding18" },
		{ "include": "#kwsMonsterModding19" }, { "include": "#kwsMonsterModding20" },
		{ "include": "#kwsClassModding1" }, { "include": "#kwsClassModding2" },
		{ "include": "#kwsClassModding3" }, { "include": "#kwsClassModding4" },
		{ "include": "#kwsTerrainModding1" }, { "include": "#kwsTerrainModding2" },
		{ "include": "#kwsTerrainModding3" }, { "include": "#kwsRitualModding1" },
		{ "include": "#kwsRitualModding2" }, { "include": "#kwsRitualModding3" },
		{ "include": "#kwsRitualModding4" }, { "include": "#kwsRitualModding5" },
		{ "include": "#kwsRitualModding6" },
		
		{
			"include": "#strings"
		},
		
		{
			"include": "#comments"
		},
		{
			"include": "#digits"
		}
	],
	"repository": {

		"strings": {
			"name": "string.quoted.double.c4m",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.c4m",
					"match": "\\\\."
				}
			]
		},

		"kwsWeaponModding1": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(icon|description|modprio|playercolor|tablecolor|guibordertex|guiborder2tex|guiborder3tex|guiborderthintex|guibordersquaretex|guititletex|guibartex|guibardarktex|guihbartex|guisbartex)\\b"
			}]
		},
		"kwsWeaponModding2": {
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(newweapon|selectweapon|clearwspec|init|trgrank|mundane|dmgtype|dmg|aoe|look|sound|flylook|flysound|flymode|yellow|blue|next|nextdmg)\\b"
			}]
		},
		"kwsWeaponModding3": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(nostr|shieldneg|an|playercolor|holykill|sweep|fullsweep|drain|oneshot|arrow|lob|scatter|rangepen|reload1|reload2|reload3|reanimate|ghoulify|dispossess|mutate|replicate)\\b"
			}]
		},
		"kwsWeaponModding4": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(easymr|mr|hardmr|morale|hardmorale|stressist|poison|drown|flying|ethereal|undead|mind|inanimate|stone|huge|large|onlyenemy|onlyfriend|affectmale|affecthuman|affectundead|affectanimal)\\b"
			}]
		},
		"kwsMonsterModding1": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(selectmonster|newmonster|name|descr|spr1|spr2|copyspr|look|lookslike|neverturn|clearspec|clearweapons|clearmove|size1x1|size2x2|hp|str|mor|mr|armor|rank|rearpos|frontpos|extraeyes)\\b"
			}]
		},
		"kwsMonsterModding2": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(huge|flying|float|teleport|immobile|stationary|slow|fast|mountain|swamp|snow|water|noland|wateronly|tunnel|winteridle|stupid|horror|battlefast|battleslow|wall|passwall|trample|tramplexsize|loner)\\b"
			}]
		},
		"kwsMonsterModding3": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(aggressive|swamp1|swamp2|desert1|desert2|deadforest1|deadforest2|mountain1|mountain2|jungle1|jungle2|money1|money2|forest1|forest2|temple1|temple2|coastal|south1|south2|nonruin1|north1|gates1|void2)\\b"
			}]
		},
		"kwsMonsterModding4": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(aihold|aigoldrally|aipowcom1|aipowcom2|aipowcom3|ainofollower|aimaxshop|wander|infwander|hadeswander|maxsinners|forestheart|likestoburn|makeruin|changetemp)\\b"
			}]
		},
		"kwsMonsterModding5": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(nametype|human|animal|troll|coldblood|undead|inanimate|stonebeing|noleader|unique|unimportant|setcreator|immortal|immortalap|reformloc|reformdestroy|planeshift|noplanecamo|homeplane|banishsurv|statue)\\b"
			}]
