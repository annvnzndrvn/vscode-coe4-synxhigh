			}]
		},
		"kwsMonsterModding11": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(mirror|revertmirror|phantasm|releasephant|releaserate|releasespell|mirrorammo)\\b"
			}]
		},
		"kwsMonsterModding12": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(meleeambush|spread|deployoutside)\\b"
			}]
		},
		"kwsMonsterModding13": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(awe|fear|seduceaura|leadership|localleadership|fireaura|coldaura|shockaura|spikes|poisoncloud|poisonexpl|fireexpl|shardexpl|putridexpl|diseasecloud|mindexpl|nozoc|berserker|holy|digest|incorporate|hpoverflow|maxsum|acidblood)\\b"
			}]
		},
		"kwsMonsterModding14": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(growhp|shrinkhp|growtime|growoffs|firstshape|secondshape|landshape|watershape)\\b"
			}]
