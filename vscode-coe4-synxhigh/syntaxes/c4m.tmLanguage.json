		"kwsClassModding1": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(selectclass|newclass|setclassname|classdescr)\\b"
			}]
		},
		"kwsClassModding2": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(hometerr|clearstartterr|addstartterr|reqterr)\\b"
			}]
		},
		"kwsClassModding3": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(goldbonus|ironbonus|tradebonus)\\b"
			}]
		},
		"kwsClassModding4": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(clearstartunits|addstartunits|addstartcom|setmaincom|clearrec|addunitrec|addmercrec|addcomrec|templerec|libraryrec|templebonusdescr|libbonusdescr|stdtroops|nostdtroops|recwizchance|recherochance|recasschance)\\b"
			}]
		},
		"kwsTerrainModding1": {
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(selectterr|spr|clearspec|clearfort|name|apcost|ownable|rare|gold|iron|trade|herbs|fungus|weed|sacr|dead|hands|goldboost|ironboost|tradeboost|harvest|pop|relic1|relic2|relicm1|relicm2)\\b"
			}]
		},
		"kwsTerrainModding2": {
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(forest|mountain|water|cave|mine|burnable|snowok|desertok|eastcoast|deepspawn|nostart|settlement|library1|library2|temple|el|hoburg|useable|farsight|nosight|invseason|spreadcold|melts|smoke|invisible)\\b"
			}]
		},
		"kwsTerrainModding3": {
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(voidret|earthgate|airgate|watergate|firegate|citadel|siegeable|woodencitadel|forestcitadel|walls|highwalls|widebattlements|woodengate|irongate|enchantedgate|extracover|sidegates|widegate|nostdrec)\\b"
			}]
		},
		"kwsRitualModding1": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(newritpow|newritual|selectritual|ritpow|level|addstring|clearritspec|soundfx|centercaster|closewin|terr|levelreq|nomonreq|nomonworldreq|nofortreq|fortreq|hasportalreq|planereq|enchherereq|benrestrict|resrestrict|resreq)\\b"
			}]
		},
		"kwsRitualModding2": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(cost|apcost|varcost|rebatefx25|rebatefx50|rebateterr20|rebateterr50|rebatelvl|summoning|farsummon|defctrl|setplayer|failplayer|raisedead|raiseanimals|simulacrum|terrboost|terrscale50|sacrscale|sum0chance|sum1chance)\\b"
			}]
		},
		"kwsRitualModding3": { 
