			}]
		},
		"kwsRitualModding4": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(centerloc|trollifyloc|reducetown|destroyterr|alterloc|squarespec|scryloc|scatterscry|teleportloc|makeportal|squareench|updatehome|planeswap|unfollowtarg|transformtarg|setvar|bentarg|bentargvar|bentargall|gainbless)\\b"
			}]
		},
		"kwsRitualModding5": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(ornext|ornext2|ornext3|mirrortarg|fillmirror|killtarg)\\b"
			}]
		},
		"kwsRitualModding6": { 
				"patterns": [{
				"name": "keyword.control.c4m",
				"match": "\\b(levelup|levelupmon|promotion|forgetrits|free|start|nostart|rarestart|nexttoo|noundead|nodemon|notforpoor|gainrit|newrit|newspell1|newspell2|newspell3)\\b"
			}]
		},
		"comments": {
				"patterns": [{
				"name": "comment.c4m",
				"match": "\\#(.*)"
			}]
		},

		"digits": {
				"patterns": [{
				"name": "constant.language.c4m",
				"match": "[0-9]"
			}]
		}

		
	}
}